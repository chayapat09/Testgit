radius = 50:50:1500;
out3Pics(1:length(radius)) = 0;
out5Pics(1:length(radius)) = 0;
out9Pics(1:length(radius)) = 0;
for i = 1:length(radius)
    out5Pics(i) = TangentDistorsion( , ,radius(i));
    out3Pics(i) = TangentDistorsion( , ,radius(i));
    out3Pics(i) = TangentDistorsion( , ,radius(i));
end

